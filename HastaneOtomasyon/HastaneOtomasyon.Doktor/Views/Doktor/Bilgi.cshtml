@{
}
@using EntityLayer
@model List<Hasta>

<div class="container">
    <div class="row">
        <div class="col-md col-sm">
            @foreach (var item in Model)
            {
               <p><b>Hasta TC: </b> @item.HastaTc </p>
                <br />
                <p><b>Hasta Adı Soyadı: </b>@item.HastaAdi&nbsp;@item.HastaSoyadi</p>
                <br />
            }
        </div>
    </div>
</div>
