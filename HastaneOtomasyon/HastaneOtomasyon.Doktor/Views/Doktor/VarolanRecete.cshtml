@{
}
@using EntityLayer
@model List<Recete>

<div class="container">
    <div class="row">
        <div class="col-md col-sm">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">Reçete Id</th>
                        <th scope="col">Tanı ve Reçete</th>
                    </tr>
                </thead>
                @foreach (var item in Model)
                {

                    <tbody>
                        <tr>
                            <td>@item.ReceteId</td>
                            <td>@item.ReceteIcerik</td>
                        </tr>

                    </tbody>
                }
            </table>
        </div>
    </div>
</div>